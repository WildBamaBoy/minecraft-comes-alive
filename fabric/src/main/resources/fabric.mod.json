  "environment": "*",
  "entrypoints": {
    "main": [
      "mca.MCAFabric"
    ],
    "client": [
      "mca.MCAClient"
    ]
  },
  "mixins": [
    "mca.mixin.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.3",
    "java": ">=8",
    "fabric-api-base": ">=0.1.0"
  }
