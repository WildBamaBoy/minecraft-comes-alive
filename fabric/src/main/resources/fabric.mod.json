  "authors": [
    "WildBamaBoy", "SheWolfDeadly"
  ],
  "contact": {
    "sources": "https://github.com/Luke100000/minecraft-comes-alive",
    "issues": "https://github.com/Luke100000/minecraft-comes-alive/issues"
  },
  "license": "GNU",
  "icon": "mca.png",
